{
  "name": "soneium-automation",
  "version": "7.0.0",
  "description": "Автоматизация операций в сети Soneium с случайным выбором модулей и ключей",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "postinstall": "npm run build",
    "aave": "tsx src/run-module.ts aave",
    "arkada-checkin": "tsx src/run-module.ts arkada-checkin",
    "collector": "tsx src/run-module.ts collector",
    "jumper": "tsx src/run-module.ts jumper",
    "lootcoin": "tsx src/run-module.ts lootcoin",
    "morpho": "tsx src/run-module.ts morpho",
    "sake-finance": "tsx src/run-module.ts sake-finance",
    "stargate": "tsx src/run-module.ts stargate",
    "untitled-bank": "tsx src/run-module.ts untitled-bank",
    "revoke": "tsx src/run-module.ts revoke",
    "redbutton-noob": "tsx src/run-module.ts redbutton-noob",
    "harkan": "tsx src/run-module.ts harkan",
    "velodrome": "tsx src/run-module.ts velodrome",
    "wowmax": "tsx src/run-module.ts wowmax",
    "captain-checkin": "tsx src/run-module.ts captain-checkin"
  },
  "keywords": [
    "soneium",
    "blockchain",
    "automation",
    "defi",
    "typescript"
  ],
  "author": "Soneium Automation",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "exceljs": "^4.4.0",
    "fast-deep-equal": "^3.1.3",
    "https-proxy-agent": "^7.0.6",
    "prompts": "^2.4.2",
    "readline-sync": "^1.4.10",
    "socks-proxy-agent": "^8.0.5",
    "viem": "^2.39.3",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/readline-sync": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "js-yaml": "^4.1.1",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "glob": "^11.1.0",
    "rimraf": "^6.1.0",
    "lodash.isequal": "npm:fast-deep-equal@^3.1.3",
    "unzipper": "^0.12.3",
    "@isaacs/brace-expansion": "^5.0.1"
  }
}
